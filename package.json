{
	"name": "volyfequickdev",
	"displayName": "VolyFEQuickDev",
	"description": "A quick dev tool to be used in Voly FE codebase (only works on Svelte file)",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.82.0"
	},
	"publisher": "Voly",
	"pricing": "Free",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "volyfequickdev.enable",
				"title": "Voly Frontend Quick Dev: Enable"
			},
			{
				"command": "volyfequickdev.disable",
				"title": "Voly Frontend Quick Dev: Disable"
			},
			{
				"command": "volyfequickdev.remove-dev-builds-folder",
				"title": "Voly Frontend Quick Dev: Remove The Dev Builds Folder"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "volyfequickdev-devBuildsFolder-explorer",
					"title": "Voly Dev Builds Folder Explorer",
					"icon": "https://ui.voly.co.uk/theme/voly/images/Logomark_Primary.svg"
				}
			]
		},
		"views": {
      "explorer": [
        {
          "id": "volyfequickdev-devBuildsFolder-explorer",
          "name": "Voly Dev Builds Folder Explorer"
        }
      ]
    }
	},
	"repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/voly/local-development.git"
  },
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/cors": "^2.8.14",
		"@types/express": "^4.17.18",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.82.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.4",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.2"
	}
}
